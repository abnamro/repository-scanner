{
  "name": "resc-frontend",
  "version": "3.0.0",
  "author": "ABN AMRO Bank",
  "description": "Repository Scanner Frontend",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ABNAMRO/repository-scanner"
  },
  "scripts": {
    "serve": "vite preview",
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "npm run ut",
    "ut": "vitest --coverage",
    "coverage": "npm run ut",
    "build-only": "vite build",
    "tsc": "npm run type-check",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint --max-warnings 1 --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore .",
    "lint:autofix": "eslint --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore --fix .",
    "lintcss": "stylelint src/**/*.{scss,css}",
    "lintcss:autofix": "stylelint src/**/*.{scss,css} --fix",
    "gen:responses": "openapi-typescript ./openapi.json -o ./src/services/schema.d.ts"
  },
  "dependencies": {
    "@browsery/util": "^0.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.5",
    "@fortawesome/free-solid-svg-icons": "^6.5",
    "@fortawesome/vue-fontawesome": "^3.0.5",
    "axios": "^1.6.2",
    "axios-retry": "^4.0.0",
    "bootstrap": "^5.3.2",
    "bootstrap-vue-next": "^0.15.5",
    "chart.js": "^4.4.1",
    "jose": "^4.5.0",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "qs": "^6.10.3",
    "vue": "^3.3.9",
    "vue-chartjs": "^5.2.0",
    "vue-loader": "^17.3.1",
    "vue-multiselect": "^3.0.0-beta.3",
    "vue-router": "^4.2.5",
    "vue-sidebar-menu": "^5.2.12"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.3",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^18.18.5",
    "@types/qs": "^6.9.10",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "@vue/compiler-sfc": "^3.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^23.0.0",
    "npm-run-all2": "^6.1.1",
    "openapi-typescript": "^6.7.1",
    "postcss-nesting": "^12.0.2",
    "prettier": "^3.2.5",
    "sass": "^1.69.7",
    "stylelint": "^16.1.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "typescript": "~5.2.0",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^4.4.11",
    "vite-plugin-node-polyfills": "^0.17.0",
    "vite-plugin-stylelint": "^5.3.1",
    "vitest": "^0.34.6",
    "vitest-sonar-reporter": "^0.5.0",
    "vue-tsc": "^1.8.19"
  }
}
